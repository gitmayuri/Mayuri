    function GetSessionDetails() {
        debugger;
        $.ajax({
            type: "GET",
            url: "@(Url.Action("GetSessionDetails", "Session"))",
            dataType: "JSON",
            cache: false,
            success: function (data) {
              //Student Details
            $('[id*=lblstdId]').text(data[0].Id);
            $('[id*=lblfname]').text(data[0].FullName);
            $('[id*=lblgender]').text(data[0].Gender);
            $('[id*=lbldob]').text(ToJavaScriptDate(data[0].DOB));
            $('[id*=lbladdress]').text(data[0].Address);
            $('[id*=lblcity]').text(data[0].City);
            $('[id*=lblpincode]').text(data[0].Pincode);
            //CDf Details
            $('[id*=lblCdfId]').text(data[0].CDFId);
            $('[id*=lblcdfName]').text(data[0].CDFName);
            $('[id*=lblcdfGender]').text(data[0].CDFGender);
            $('[id*=lblcdfContact]').text(data[0].CDFContact);
            $('[id*=lblcdfEmail]').text(data[0].CDFEmail);
            $('[id*=lblcdfLevel]').text(data[0].CDFLevels);
           
            //Shadow Details
            if (data[0].ShadowId == 0) {
                $("#lblshdowmsg").text("Shadow CDF is not assign.");
                $("#tblshadow").hide();
            }
            else {
                $("#tblshadow").show();
                $("#lblshdowmsg").hide();
                $('[id*=lblshadowId]').text(data[0].ShadowId);
                $('[id*=lblshadowName]').text(data[0].ShadowName);
                $('[id*=lblshadowGender]').text(data[0].ShadowGender);
                $('[id*=lblshadowContact]').text(data[0].ShadowContact);
                $('[id*=lblshadowEmail]').text(data[0].ShadowEmail);
                $('[id*=lblshadowLevel]').text(data[0].ShadowLevel);
              }
            
               //Session Details
               $('[id*=lblSessionDate]').text(ToJavaScriptDate(data[0].SessionDate));
               $('[id*=lblSessionTime]').text(data[0].SesstionTime);
               $('[id*=lblSessionAddress]').text(data[0].SesAddress);
               $('[id*=lblSessionStatus]').text(data[0].SessionStatus);
               $('[id*=lblSessionCdfResponse]').text(data[0].SesCDFResponce);
            
        },
        error: function (data) {
            responseText = jQuery.parseJSON(data.responseText);
            alert(responseText.Message);

        }
    });
    }

