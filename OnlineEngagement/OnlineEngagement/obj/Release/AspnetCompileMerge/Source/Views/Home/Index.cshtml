    function ToJavaScriptDate(value) {

        var pattern = /Date\(([^)]+)\)/;
        var results = pattern.exec(value);
        var dt = new Date(parseFloat(results[1]));
        //var dt2= (dt.getMonth() + 1) + "/" + dt.getDate() + "/" + dt.getFullYear();
        //  var dt1 = dt.getFullYear() + "/" + (dt.getMonth() + 1) + "/" + dt.getDate();
        var dt1 = dt.getDate() + "/" + (dt.getMonth() + 1) + "/" + dt.getFullYear();
        return dt1;
    }


    function SaveSession() {

        $.ajax({
            // url: '/Home/PostSessionCDFDetails',
            type: 'POST',
            url: "@(Url.Action("PostSessionCDFDetails", "Home"))",
            dataType: 'json',
            cache: false,
            data: {

                CDFId: $('[id*=hfCDFId]').val(),
                StudID: $('[id*=hfStudId]').val(),
                sessionVenue: $('[id*=txtVenue]').val(),
                sessionPinCode: $('[id*=txtSessionPin]').val(),
                sessionArea: $('[id*=txtArea]').val(),
                sessionDate: $('[id*=txtSessionDate]').val(),
                SessionTime: $('[id*=txtSessionTime]').val()
            },
            success: function (data) {

                if (data == true) {
                    alert("Data Save");
                }
                else { alert("Data Not Save"); }
            },
            error: function (response) {
                responseText = jQuery.parseJSON(response.responseText);
                alert(responseText.Message);

            }
        });


    }

    function SubmitReassignTest() {
        var TestId = $("#ddlTest option:selected").val();
        var TestCompCandId = $('[id*=hfStudentIdToReassign]').val();
        $.ajax({
            // url: '/Home/PostSessionCDFDetails',
