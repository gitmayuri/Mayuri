    function ApproveGraph() {
        alert("Approve");
        $.ajax({
            type: 'POST',
            url: "@(Url.Action("PostApproveGraph", "Home"))",
            dataType: 'json',
            cache: false,
            data: { TCompCandID: TCompCandID },
            success: function (data) {
                alert(data);
                $('[id*=graphApprove]').prop("disabled", true);
                //  ShowTestCompStudList();
                //  $(document).load(window.location.href + " #myModalTestCompletedCand");
            },
            error: function (response) {
                responseText = jQuery.parseJSON(response.responseText);
                alert(responseText.Message);

            }
        });
    }

    function BindTest() {
        var candIdTOGetTest = $('[id*=hfStudentIdToReassign]').val();
        $.ajax({
            type: "GET",
            url: "@(Url.Action("BindTest", "Home"))",
            dataType: "JSON",
            data: { candIdTOGetTest: candIdTOGetTest },
            cache: false,
            success: function (data) {
                var items = '<option>--Select--</option>';
                $.each(data, function (i, state) {
                    items += "<option value='" + state.Value + "'>" + state.Text + "</option>";
                });
                $('#ddlTest').html(items);
            },
