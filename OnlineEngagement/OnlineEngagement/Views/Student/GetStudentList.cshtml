@model IEnumerable<OnlineEngagement.Models.UserDetail>

@{
    ViewBag.Title = "";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="content-wrapper">
    <div id="widget-list">
        <div id="row">
            <div class="col-md-12 widget-holder">
                <div class="widget-bg">
                    @*<h3 class="page-header">Product Purchased Student List</h3>*@
                    <div class="row">
                        <div class="col-lg-12">                        
                            @*<div class="table-responsive">*@
                                <div class="widget-body clearfix">
                                    <table class="table table-striped table-responsive" data-toggle="datatables" border="1">
                                        <thead>
                                            <tr style="background-color: rgb(4, 41, 68);color:white;">
                                                <th style="display:none;"></th>
                                                <th>
                                                    Student Name
                                                    @*@Html.DisplayNameFor(model => model.Name)*@
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Gender)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.EmailId)
                                                </th>
                                                <th>
                                                    Mobile
                                                    @*@Html.DisplayNameFor(model => model.Contact)*@
                                                </th>

                                                <th>
                                                    @Html.DisplayNameFor(model => model.Address)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.City)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Pincode)
                                                </th>

                                                @*<th>
                                                    @Html.DisplayNameFor(model => model.RegDate)
                                                </th>*@
                                                @*<th>
                                                    @Html.DisplayNameFor(model => model.TestStatus)
                                                </th>*@

                                                <th>
                                                    Details

                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td style="display:none;"></td>
                                                    <td style="width:50px;">
                                                        @Html.DisplayFor(modelItem => item.Name)
                                                    </td>
                                                    <td style="width:50px;">
                                                        @Html.DisplayFor(modelItem => item.Gender)
                                                    </td>

                                                    @*<td>
                                                            @Html.DisplayFor(modelItem => item.BirthDate)
                                                        </td>*@
                                                    <td style="width:30px;">
                                                        @Html.DisplayFor(modelItem => item.EmailId)
                                                    </td>

                                                    <td style="width:50px;">
                                                        @Html.DisplayFor(modelItem => item.Contact)
                                                    </td>
                                                    <td style="width:100px;">
                                                        @Html.DisplayFor(modelItem => item.Address)
                                                    </td>
                                                    <td style="width:50px;">
                                                        @Html.DisplayFor(modelItem => item.City)
                                                    </td>
                                                    <td style="width:50px;">
                                                        @Html.DisplayFor(modelItem => item.Pincode)
                                                    </td>

                                                    @*<td>
                                                            @Html.DisplayFor(modelItem => item.RegDate)
                                                        </td>*@
                                                    @*<td>
                                                        @Html.DisplayFor(modelItem => item.TestStatus)
                                                    </td>*@
                                                    <td style="width:50px;color:Blue;font-weight:bold;text-decoration:underline;">
                                                       @Html.ActionLink("Assign CDF", "StudentMoreInfo", new { id = item.Id, pincode = item.Pincode, studName = item.Name, email= item.EmailId, City= item.City })
                                                    </td>
                                                    
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
@*</div>*@
                                     
                                                                  
                           

